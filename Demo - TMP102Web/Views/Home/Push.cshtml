
@{
    ViewBag.Title = "Push";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-4">
        <h2>TMP102 Readings</h2>
        <table>
            @using Demo___TMP102Web.Models
            @Model TMP102

            @for (int i = 0; i < Model.Values.Count; i++)
            {
                <tr>
                    <td>
                        @(i + 1)
                    </td>
                    <td>
                        @Model.Values[i]
                    </td>
                </tr>
            }
        </table>
    </div>
</div>

